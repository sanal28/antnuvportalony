                $("#ddlLeadGender").click();
                $("#ddlLeadGender").attr('disabled', true);
                $("#txtLeadEmail").val(resultVals[0]["EmailId"]).attr('disabled', true);
                $("#ddlLeadEmpType").val(resultVals[0]["EmployeeType"]);
