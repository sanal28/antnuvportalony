                $("#ddlLeadVisaType").attr('disabled', true);
                $("#txtLeadStreet").val(resultVals[0]["Street"]).attr('disabled', true);
                $("#txtLeadCity").val(resultVals[0]["City"]).attr('disabled', true);
                $("#txtLeadState").val(resultVals[0]["State"]).attr('disabled', true);
                $("#txtLeadCountry").val(resultVals[0]["Country"]).attr('disabled', true);
                $("#txtLeadZip").val(resultVals[0]["Zip"] == 0 ? "" : resultVals[0]["Zip"]).attr('disabled', true);
