            $('#hdnLeadId').val(LeadId);
            $('#ddlLeadTypes').val(TypeId);
            $('#ddlLeadTypes').click();
            $('.headingText').text("Please Confirm Lead Type");
            var dialog = document.getElementById('dlgConfirmtype');
            dialog.showModal();
        }
        else {
            var items = [];
            $("#hideRow").hide();
            $('.headingText').text("Assign Candidate");                     
            if (Email == 'NA') {
                items.push('Email');
            }
            if (Mobile == 'NA') {
                items.push('Mobile');
            }
            if (Country == "") {
                items.push('Country');
            }
            if (EmpType == 'NA') {
                items.push('Employee Type');
            }
            if (EmpType == 'Experienced' && Exp == 0)
            {
                items.push('Experience in months');
            }
            if (LeadSkill ==0) {
                items.push('Skills');
            }
            $("#enterMsg").removeClass("lblError");
            $("#enterMsg").addClass("lblMessage");
            $("#enterMsg").text("Please enter "+items+" before assigning Candidate");
            var dialog = document.getElementById('dlgConfirmtype');
            dialog.showModal();
        }
    }
