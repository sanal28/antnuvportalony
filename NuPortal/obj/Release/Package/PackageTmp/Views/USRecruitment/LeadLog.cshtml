    function ShowPopUp(LeadId, TypeId, Email, Mobile, Country, EmpType, Exp,LeadSkill) {
        
        if (Email != 'NA' && Mobile != 'NA' && Country != "" && LeadSkill!=0 && EmpType != 'NA' && ((EmpType == 'Fresher' && Exp == 0) || (EmpType == 'Experienced' && Exp != 0))) {
            $('#enterMsg').text("");
