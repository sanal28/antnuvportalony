<script>

    var dictOpenInterviewArray = {};
    var dictClosedInterviewArray = {};
    var IsPrimaryLoad = true;
    var CurrentPageId = 1;
    var CurrentPageIdCompleted = 1;
    var RowCount = 10;
    var PageCount = 0;
    var PageCountCompleted = 0;
    var OffsetVal = 0;
    var loopVal = 0;
    var classId = 0;
    $(document).ready(function () {
        dictOpenInterviewArray = { "Field1Xml": "", "Field2Xml": "", "Field3Xml": "", "Field4Xml": "", "Field5Xml": "", "Field6Xml": "" };
        dictClosedInterviewArray = { "Field1Xml": "", "Field2Xml": "", "Field3Xml": "", "Field4Xml": "", "Field5Xml": "", "Field6Xml": "", "Field7Xml": "" };
        $(".slidingDivGrid").hide();
        $(".show_hideGrid").show();
        $('.show_hideGrid').click(function () {
            $(".slidingDivGrid").not($(this).closest(".requestgridheader").find('.slidingDivGrid')).hide();
            $(this).closest(".requestgridheader").find('.slidingDivGrid').slideToggle();
        });
        $('.show_hideGrid').click(function (e) {
            e.preventDefault();
        });
        hideFilter();
        loadOpenInterview();
        loadOpenInterviewFilter();
        loadClosedInterview();
        loadClosedInterviewFilter();
        $('#hrefIntOpenClearFilter').click(function (e) {
            e.preventDefault();
            $(".slidingDivGrid").hide();
            $(this).closest('table').find('input.cbcheckbox:checkbox').each(function () {
                $(this).prop('checked', false);
            }); //clear all checkbox
            dictOpenInterviewArray = { "Field1Xml": "", "Field2Xml": "", "Field3Xml": "", "Field4Xml": "", "Field5Xml": "", "Field6Xml": "" };
            FirstButonClick($('.paginationNUMOpenbtn'), $('.pgNumber1'), 'paginationNUM-ACTVbtn', 1, 'paginationNUMMOREtxt-Active', 'paginationNUMMOREtxt1-Active');
            CurrentPageId = 1;
            loadOpenInterview();
            showAndHidePages(PageCount, CurrentPageId, $('.paginationbtndivNum > a'), '.pgNumber');
        });

        $('#hrefIntCompClearFilter').click(function (e) {
            e.preventDefault();
            $(".slidingDivGrid").hide();
            $(this).closest('table').find('input.cbcheckbox:checkbox').each(function () {
                $(this).prop('checked', false);
            }); //clear all checkbox
            dictClosedInterviewArray = { "Field1Xml": "", "Field2Xml": "", "Field3Xml": "", "Field4Xml": "", "Field5Xml": "", "Field6Xml": "", "Field7Xml": "" };
            FirstButonClick($('.paginationNUMCompbtn'), $('.pgNumberB1'), 'paginationNUM-ACTVbtnComp', 1,
            'paginationNUMMOREtxt-ActiveComp', 'paginationNUMMOREtxt1-ActiveComp');
            CurrentPageIdCompleted = 1;
            loadClosedInterview();
            showAndHidePages(PageCountCompleted, CurrentPageIdCompleted, $('.paginationbtndivComNum > a'), '.pgNumberB');
        });
        LoadDdls("/USRecruitment/GetInterviewType", $('#ddlInterviewType'), "InterviewTypeId", "InterviewType");
        DefineDatePicker($('#txtInterviewDate'));
        LoadDdls("/USRecruitment/GetVisaType", $('#ddlLeadVisaType'), "visatypeid", "visatype");
        LoadDdls("/USRecruitment/GetEmploymentType", $('#ddlLeadEmployeementType'), "TypeId", "TypeName");
        LoadDdls("/USRecruitment/GetLeadType", $('#ddlLeadType'), "LeadTypeId", "LeadType");
        LoadDdls("/USRecruitment/GetEmployeeType", $('#ddlLeadEmpType'), "EmpTypeId", "EmpType");
        LoadDdls("/USRecruitment/GetIndustry", $('#ddlIndustry'), "IndustryId", "IndustryName");
        LoadDdls("/USRecruitment/GetPositionType", $('#ddlPosType'), "TypeId", "TypeName");
        PreventDefaultClick();
    });

    function loadOpenInterview() {
        OffsetVal = ((CurrentPageId - 1) * RowCount);
        $.ajax({
            url: '/Home/GetGridData',
            type: "POST",
            dataType: 'json',
            data: { jsonData: JSON.stringify(dictOpenInterviewArray), Operation: 35, ProjId: 0, TypeId: 1, OffSet: OffsetVal, RowCount: RowCount },
            success: function (result) {
                var resultVals = $.parseJSON(result);
                $('#tblInterviewOpen > tbody').children("tr:gt(0)").remove();
                if (resultVals["Result"] == undefined || resultVals["Result"] == null) {
                    if (resultVals[0]["TotalRowCount"] % 10 == 0)
                        PageCount = resultVals[0]["TotalRowCount"] / 10;
                    else
                        PageCount = Math.floor(resultVals[0]["TotalRowCount"] / 10) + 1;
                    disableButtons($('#LSTbtn'), $('#NXTbtn'), $('#FRSTbtn'), $('#PRVbtn'), PageCount, CurrentPageId);
