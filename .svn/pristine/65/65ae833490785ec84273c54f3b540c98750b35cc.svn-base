    function getTextAreaData(LeadId, mode) {
        $("#lstLeadSkills").empty();
        $("#lstLeadTechnologies").empty();
        $("#lstLeadCertifications").empty();
        $.ajax({
            url: '/Home/GetEmpData',
            dataType: "json",
            type: "GET",
            data: { Id: LeadId, Operation: 87 },
            success: function (data) {
                var resultSet = $.parseJSON(data);
                if (resultSet["Result"] == undefined || resultSet["Result"] == null) {
                    var listObject = "";
                    for (var i = 0; i < resultSet.length; i++) {
                        if (resultSet[i]["Skills"] != "") {
                            if (resultSet[i]["TypeId"] == 1) {
                                listObject = $("#lstLeadSkills");
                            }
                            else if (resultSet[i]["TypeId"] == 2) {
                                listObject = $("#lstLeadTechnologies");
                            }
                            else if (resultSet[i]["TypeId"] == 3) {
                                listObject = $("#lstLeadCertifications");
                            }
                            if (listObject != undefined) {
                                listObject.append("<div class='div2'><div style='padding-left:2px;margin: 0; font-size: 11px; color: #fff; " +
                       "min-width: 50px; width:auto; float: left;'><label class='divcompetancy'>" + resultSet[i]["Skills"] +
                        "</label></div><div id='closeButton' style='margin: 0; font-size: 11px; color: #fff; height: 10px;  width:10px; float: left; '>" +
                        "</div></div>");
                            }
                        }
                    }
                    PreventDefaultClick();
                }
            },
            error: function (err) {
            }
        });
    }
</script>


