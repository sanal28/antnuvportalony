<script>
    var dictSubmissionsOpen = {};
    var dictSubmissionsRes = {};
    var FileInputCount = 0;
    var IsPrimaryLoad = true;
    var CurrentPageId = 1;
    var CurrentPageIdResponded = 1;
    var RowCount = 10;
    var PageCount = 0;
    var PageCountResponded = 0;
    var OffsetVal = 0;
    var loopVal = 0;
    var classId = 0;
    dictSubmissionsOpen = { "Field1Xml": "", "Field2Xml": "", "Field3Xml": "", "Field4Xml": "", "Field5Xml": ""};
    dictSubmissionsRes = { "Field1Xml": "", "Field2Xml": "", "Field3Xml": "", "Field4Xml": "", "Field5Xml": "", "Field6Xml": ""};
    $(document).ready(function () {
        $(".slidingDivGrid").hide();
        $(".show_hideGrid").show();
        hideFilter();
        $('.show_hideGrid').click(function () {
            $(".slidingDivGrid").not($(this).closest(".requestgridheader").find('.slidingDivGrid')).hide();
            $(this).closest(".requestgridheader").find('.slidingDivGrid').slideToggle();
        });
        $('.show_hideGrid').click(function (e) {
            e.preventDefault();
        });
        PreventDefaultClick();
        SubmissionsOpen();
        SubmissionsOpenFilter();
        SubmissionsResponded();
        SubmissionsRespondedFilter();
        $('#hrefSubOpenClearFilter').click(function (e) {
            e.preventDefault();
            $(".slidingDivGrid").hide();
            $(this).closest('table').find('input.cbcheckbox:checkbox').each(function () {
                $(this).prop('checked', false);
            }); //clear all checkbox
            dictSubmissionsOpen = { "Field1Xml": "", "Field2Xml": "", "Field3Xml": "", "Field4Xml": "", "Field5Xml": "" };
            FirstButonClick($('.paginationNUMOpenbtn'), $('.pgNumber1'), 'paginationNUM-ACTVbtn', 1, 'paginationNUMMOREtxt-Active', 'paginationNUMMOREtxt1-Active');
            CurrentPageId = 1;
            SubmissionsOpen();
            showAndHidePages(PageCount, CurrentPageId, $('.paginationbtndivNum > a'), '.pgNumber');
        });
        $('#hrefSubResClearFilter').click(function (e) {
            e.preventDefault();
            $(".slidingDivGrid").hide();
            $(this).closest('table').find('input.cbcheckbox:checkbox').each(function () {
                $(this).prop('checked', false);
            }); //clear all checkbox
            dictSubmissionsRes = { "Field1Xml": "", "Field2Xml": "", "Field3Xml": "", "Field4Xml": "", "Field5Xml": "", "Field6Xml": "" };
            FirstButonClick($('.paginationNUMCompbtn'), $('.pgNumberB1'), 'paginationNUM-ACTVbtnComp', 1,
            'paginationNUMMOREtxt-ActiveComp', 'paginationNUMMOREtxt1-ActiveComp');
            CurrentPageIdResponded = 1;
            SubmissionsResponded();
            showAndHidePages(PageCountResponded, CurrentPageIdResponded, $('.paginationbtndivComNum > a'), '.pgNumberB');
        });
        LoadDdls("/USRecruitment/GetVisaType", $('#ddlLeadVisaType'), "visatypeid", "visatype");
        LoadDdls("/USRecruitment/GetEmploymentType", $('#ddlLeadEmployeementType'), "TypeId", "TypeName");
        LoadDdls("/USRecruitment/GetLeadType", $('#ddlLeadType'), "LeadTypeId", "LeadType");
        LoadDdls("/USRecruitment/GetEmployeeType", $('#ddlLeadEmpType'), "EmpTypeId", "EmpType");
        LoadDdls("/USRecruitment/GetIndustry", $('#ddlIndustry'), "IndustryId", "IndustryName");
        LoadDdls("/USRecruitment/GetPositionType", $('#ddlPosType'), "TypeId", "TypeName");
    });
    function SubmissionsOpen() {
        OffsetVal = ((CurrentPageId - 1) * RowCount);
        $.ajax({
            url: '/Home/GetGridData',
            type: "POST",
            dataType: 'json',
            data: { jsonData: JSON.stringify(dictSubmissionsOpen), Operation: 33, ProjId: 0, TypeId: 1, OffSet: OffsetVal, RowCount: RowCount },
            success: function (result) {
                var resultVals = $.parseJSON(result);
                $('#tblOpenSubmissions > tbody').children("tr:gt(0)").remove();
                if (resultVals["Result"] == undefined || resultVals["Result"] == null) {
                    if (resultVals[0]["TotalRowCount"] % 10 == 0)
                        PageCount = resultVals[0]["TotalRowCount"] / 10;
                    else
                        PageCount = Math.floor(resultVals[0]["TotalRowCount"] / 10) + 1;
                    disableButtons($('#LSTbtn'), $('#NXTbtn'), $('#FRSTbtn'), $('#PRVbtn'), PageCount, CurrentPageId);
                    showAndHidePages(PageCount, CurrentPageId, $('.paginationbtndivNum > a'), '.pgNumber');
                    for (var i = 0; i < resultVals.length; i++) {
                        SubmittedOn = FormatDate(resultVals[i]["CreatedDate"], "mm/dd/yyyy");
                        $('#tblOpenSubmissions').append('<tr class="requestgriddiv2" ><td class="requestgridsubheader">' +
                            resultVals[i]["ClientName"] + '</a></div></td><td class="requestgridsubheader" ><div class="documentlinkdiv">'+
                            '<a href="#" class="hreflink" onclick="loadLeadData(\'' + resultVals[i]["name"] + '\',\'' + resultVals[i]["LeadId"] + '\');">' + resultVals[i]["name"] + '</td></a><div>' +
                            '<td class="requestgridsubheader"><div class="documentlinkdiv"><a href="#" class="hreflink" onclick="loadOppData(\'' + resultVals[i]["PositionTitle"] + '\',\'' + resultVals[i]["OppId"] + '\')">' + resultVals[i]["PositionTitle"] + '</a></div></td><td class="requestgridsubheader" >' + SubmittedOn +
                            '</td><td class="requestgridsubheader">' + resultVals[i]["createdBy"] + '</td><td class="requestgridsubheader">' +
                            '<div class="documentlinkdiv"><a href="#" onClick="loadAssociateOpporuniy(\'' + resultVals[i]["SubmissionId"] + '\')">' +
                            "Submitted Form" + '</a></div></td><td class="requestgridsubheader"></td></tr>');
                    }
                    $('#divPage').show();
                    PreventDefaultClick();
                }
                else
                {
                        $('#divPage').hide();
                        $('#tblOpenSubmissions').append("<tr><td colspan='8' class='NoRecordsFound'>No Records Found</td></tr>");
                }
            },
            error: function (err) {
            }
        });
    }
    function SubmissionsOpenFilter() {
        $.ajax({
            url: '/Home/GetGridData',
            type: "POST",
            dataType: 'json',
            data: { jsonData: JSON.stringify(dictSubmissionsOpen), Operation: 33, ProjId: 0, TypeId: 1, OffSet: 0, RowCount: 0 },
            success: function (result) {
                var resultVals = $.parseJSON(result);
                if (resultVals["Result"] == undefined || resultVals["Result"] == null) {
                    for (var i = 0; i < resultVals.length; i++) {
                        SubmittedOn = FormatDate(resultVals[i]["CreatedDate"], "mm/dd/yyyy");
                        if (IsPrimaryLoad) { //condition to load checkbox only in first load, not in searched data load
                            CreateCheckBox($('#cbSubOpenClientName'), resultVals[i]["ClientName"], 'Field1Xml');
                            CreateCheckBox($('#cbSubOpenCanName'), resultVals[i]["name"], 'Field2Xml');
                            CreateCheckBox($('#cbSubOpenPostTitle'), resultVals[i]["PositionTitle"], 'Field3Xml');
                            CreateCheckBox($('#cbSubOpenSubOn'), SubmittedOn, 'Field4Xml');
                            CreateCheckBox($('#cbSubOpenSubBy'), resultVals[i]["createdBy"], 'Field5Xml');
                            DefineCheckboxClicks();
                        }
                    }
                }
            },
            error: function (err) {
            }
        });
    }

    function SubmissionsResponded() {
        OffsetVal = ((CurrentPageIdResponded - 1) * RowCount);
        $.ajax({
            url: '/Home/GetGridData',
            type: "POST",
            dataType: 'json',
            data: { jsonData: JSON.stringify(dictSubmissionsRes), Operation: 34, ProjId: 0, TypeId: 1, OffSet: OffsetVal, RowCount: RowCount },
            success: function (result) {
                var resultVals = $.parseJSON(result);
