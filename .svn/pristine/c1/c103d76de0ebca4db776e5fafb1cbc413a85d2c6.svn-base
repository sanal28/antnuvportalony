                $("#txtCreatedBy").val(resultVals[0]["CreatedBy"]).attr('disabled', true);;
                $("#txtOppStatus").val(resultVals[0]["OpportunityStatus"]).attr('disabled', true);;
                $('#filehidden').val(resultVals[0]["Attachments"]);
                $('#txtSecondarySkills').hide();
                $('#txtReqDoc').hide();
                $('#divOppReqDocs').empty();
                showAttachments(resultVals[0]["Attachments"], "divOppReqDocs");

                $.ajax({
                    url: '/Home/GetSelectData',
                    type: "GET",
                    async: false,
                    dataType: 'json',
                    data: { Id: OppId, Operation: 44 },
                    success: function (result) {
                        var resultVals = $.parseJSON(result);
                        if (resultVals["Result"] == undefined || resultVals["Result"] == null) {
                            for (var i = 0; i < resultVals.length; i++) {
                                if (resultVals[i]["Skills"] != "") {
                                    if (resultVals[i]["TypeId"] == false)
                                        listObject = $("#listPrimarySkills");
                                    else
                                        listObject = $("#listSecondarySkills");
                                    if (listObject != undefined)
                                        listObject.append("<div class='div2'><div style='padding-left:2px;margin: 0; font-size: 11px; color: #fff; " +
                               "min-width: 50px; width:auto; float: left;'><label class='divcompetancy'>" + resultVals[i]["Skills"] +
                                "</label></div><div style='margin: 0; font-size: 11px; color: #fff; height: 10px;  width:10px; float: left; '>" +
                                "</div></div>");
                                }
                            }
                        }
                        $.ajax({
                            url: '/Home/GetSelectData',
                            type: "GET",
                            async: false,
                            dataType: 'json',
                            data: { Id: OppId, Operation: 43 },
                            success: function (result) {
                                var resultVals = $.parseJSON(result);
                                if (resultVals["Result"] == undefined || resultVals["Result"] == null) {
                                    for (var i = 0; i < resultVals.length; i++) {
                                        if (resultVals[i]["DocType"] != "") {
                                            listObject = $("#listReqDoc");
                                            if (listObject != undefined)
                                                listObject.append("<div class='div2'><div style='padding-left:2px;margin: 0; font-size: 11px; color: #fff; " +
                                       "min-width: 50px; width:auto; float: left;'><label class='divcompetancy'>" + resultVals[i]["DocType"] +
                                        "</label></div><div style='margin: 0; font-size: 11px; color: #fff; height: 10px;  width:10px; float: left; '>" +
                                        "</div></div>");
                                        }
                                    }
                                }

                            },
                            error: function (err) {
                            }
                        });
                    },
                    error: function (err) {
                    }
                });
                PreventDefaultClick();
            },
            error: function (err) {
            }
        });
        dialogSub.showModal();
    }
</script>

