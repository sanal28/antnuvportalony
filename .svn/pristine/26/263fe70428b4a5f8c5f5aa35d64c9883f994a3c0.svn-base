                    disableButtons($('#LSTbtnOppInterViewed'), $('#NXTbtnOppInterViewed'), $('#FRSTbtnOppInterViewed'), $('#PRVbtnOppInterViewed'),
                        PageCountInterviewed, CurrentPageIdInterviewed);
                    showAndHidePages(PageCountInterviewed, CurrentPageIdInterviewed, $('.paginationbtndivOppInterViewedNum > a'), '.pgNumberOppInterViewed');
                    for (var i = 0; i < resultVals.length; i++) {
                        $('#tblOppInterviewed').append('<tr class="requestgriddiv2" ><td class="requestgridsubheader"> ' + resultVals[i]["CandidateName"] +
