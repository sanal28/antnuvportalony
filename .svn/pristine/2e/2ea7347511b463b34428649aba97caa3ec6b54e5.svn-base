                $("#txtOppWorkExp").val(resultVals[0]["WorkExperience"]).attr('disabled', true);
                $("#txtOppRate").val(resultVals[0]["Rate"] == 0 ? "" : resultVals[0]["Rate"]).attr('disabled', true);
                $("#txtOppHours").val(resultVals[0]["Hours"] == 0 ? "" : resultVals[0]["Hours"]).attr('disabled', true);
                $("#txtOppProfileType").val(resultVals[0]["ProfileType"]).attr('disabled', true);
                $("#txtOppTargetDate").val(FormatDate(resultVals[0]["TargetDate"], "mm/dd/yyyy") == "01/01/1753" ? "" : FormatDate(resultVals[0]["TargetDate"], "mm/dd/yyyy")).attr('disabled', true);
                $("#txtOppNote").val(resultVals[0]["Note"]).attr('disabled', true);
                $("#txtOppJobDes").val(resultVals[0]["JobDescription"]).attr('disabled', true);
                $("#txtOppPosTitle").val(resultVals[0]["PositionTitle"]).attr('disabled', true);
                $("#txtOppWebsite").val(resultVals[0]["Website"]).attr('disabled', true);
