                $("#txtOppStatus").val(resultVals[0]["OpportunityStatus"]).attr('disabled', true);
                $('#txtOppSecondarySkills').hide();
                $('#txtOppReqDoc').hide();
                $('#divOppReqDocs').empty();
                showAttachments(resultVals[0]["Attachments"]);

                $.ajax({
                    url: '/Home/GetSelectData',
                    type: "GET",
                    async: false,
                    dataType: 'json',
                    data: { Id: OppId, Operation: 44 },
                    success: function (result) {
                        var resultVals = $.parseJSON(result);
                        if (resultVals["Result"] == undefined || resultVals["Result"] == null) {
                            for (var i = 0; i < resultVals.length; i++) {
                                if (resultVals[i]["Skills"] != "") {
                                    if (resultVals[i]["TypeId"] == false)
                                        listObject = $("#listOppPrimarySkills");
                                    else
                                        listObject = $("#listOppSecondarySkills");
