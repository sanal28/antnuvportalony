                $("#txtOppLastUpdate").val(FormatDate(resultVals[0]["ModifiedDate"], "mm/dd/yyyy") == "01/01/1753" ? "" : FormatDate(resultVals[0]["ModifiedDate"], "mm/dd/yyyy")).attr('disabled', true);
                $('#txtOppPrimarySkills').hide();
