            },
            error: function (err) {
            }
        });
        dialogSub.showModal();
    }
    function showAttachments(attachmentURLs) {
        attachmentURLs = attachmentURLs.split('||');
        var docElement = $("#divOppReqDocs");
        for (var i = 0; i < (attachmentURLs.length - 1) ; i++) {
            CheckExtension(docElement, "", attachmentURLs[i], "", null, 1);
        }
        PreventDefaultClick();
    }
