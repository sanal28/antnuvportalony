                $("#txtOppNoOpen").val(resultVals[0]["NoOfOpenings"]).attr('disabled', true);
                $("#txtOppStreet").val(resultVals[0]["Street"]).attr('disabled', true);
                $("#txtOppCity").val(resultVals[0]["City"]).attr('disabled', true);
                $("#txtOppState").val(resultVals[0]["State"]).attr('disabled', true);
                $("#txtOppZip").val(resultVals[0]["ZipCode"]).attr('disabled', true);
                $("#txtOppContact1").val(resultVals[0]["Contact1"]).attr('disabled', true);
                $("#txtOppContact2").val(resultVals[0]["Contact2"]).attr('disabled', true);
                $("#txtOppEmail").val(resultVals[0]["Email"]).attr('disabled', true);
