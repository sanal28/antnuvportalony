    function loadCandidate(title, leadID, intCount, subCount, flag) {
        if (flag == 0)
            $(".feedsassociatebtn").show();
        else
            $(".feedsassociatebtn").hide();
        $("#lblMessage").text("");
        $('#hdnLeadId').val(leadID);
        $("#hdnModalCanSubmit").val(leadID);
        $("#hdnModalClientName").val(title);
        $('#divTaskDocs').empty();
        $("#txtSkills").hide();
        $("#txtTechnologies").hide();
        $("#txtCertifications").hide();
        $(".USRECdocumentfilebtndiv").hide();
        $("#lnkInterviewed").text("");
        $("#lnkSubmitted").text("");
        var dialog = document.getElementById("dlgCandidate");
        $('#dlgCandidate').find('.popupheadingtext')[0].innerText = title;
        $.ajax({
            url: '/Home/GetSelectData',
            type: "GET",
            dataType: 'json',
            data: { Id: leadID, Operation: 42 },
            success: function (result) {
                var resultVals = $.parseJSON(result);
                if (resultVals["Result"] == undefined || resultVals["Result"] == null) {
                    $("#lnkInterviewed").append("Interviewed (" + intCount + ")")
                    $("#lnkSubmitted").append("Submitted (" + subCount + ")");
                    $('#hdnRecStatus').val(resultVals[0]["FK_RecStatus"]);
                    $("#txtFirstName").val(resultVals[0]["FirstName"]).attr('disabled', true);
                    $("#txtLastName").val(resultVals[0]["LastName"]).attr('disabled', true);
