    function loadSubmission(SubmissionId) {
        var dialog = document.getElementById('dlgAssociateOpportunity');
        dialog.showModal();
        $.ajax({
            url: '/Home/GetSelectData',
            type: 'GET',
            dataType: 'json',
            async: false,
            data: { Id: SubmissionId, Operation: 47 },
            success: function (result) {
                var resultVals = $.parseJSON(result);
                if (resultVals["Result"] == undefined || resultVals["Result"] == null) {
                    $('#dlgAssociateOpportunity').find('.popupheadingtext')[0].innerText = resultVals[0]["name"] + ' - ' + resultVals[0]["PositionTitle"];
                    $("#txtCandidateNameSub").val(resultVals[0]["name"]).attr("disabled", "disabled");
                    $("#txtClientNameSub").val(resultVals[0]["ClientName"]).attr("disabled", "disabled");
                    $("#txtPositionTitleSub").val(resultVals[0]["PositionTitle"]).attr("disabled", "disabled");
                    $("#txtRateSub").val(resultVals[0]["Rate"] == 0 ? "" : resultVals[0]["Rate"]).attr('disabled', true);
                    $('#divFilesSub').empty();
                    for (var i = 0; i < resultVals.length; i++) {
                        if (resultVals[i]["DocType"] != null) {
                            $('#divSubmittedDocs').show();
                            if ($('#divFilesSub').find('.USRECdocument[name=' + resultVals[i]["FK_DocTypeId"] + ']').length == 0) {
                                $('#divFilesSub').append("<div class='row'>" +
                                    "<div class='col-md-3'>" +
                                        "<div class='contentfielddiv'>" +
                                            "<div class='USRECdocument docTypeId' name = '" + resultVals[i]["FK_DocTypeId"] + "'>" + resultVals[i]["DocType"] + "</div>" +
                                        "</div>" +
                                    "</div>" +
                                    "<div class='col-md-5'><div class='docDiv' id='divDocuments" + FileInputCount + "'></div></div>" +
                                "</div>");
                            }
                            showSubmissionAttachments(resultVals[i]["DocPath"], resultVals[i]["FK_DocTypeId"]);
                            FileInputCount++;
                        }
                        else
                            $('#divSubmittedDocs').hide();
                    }
                }
            },
            error: function (err) {
            }
        });

    }
    function showSubmissionAttachments(attachmentURLs, docTypeId) {
        attachmentURLs = attachmentURLs.split('||');
        //var docElement = $('#divDocuments' + FileInputCount);
        var docElement = $('#divFilesSub').find('.USRECdocument[name=' + docTypeId + ']').closest('.row').find('.docDiv');
        for (var i = 0; i <= (attachmentURLs.length - 1) ; i++) {
            CheckExtension(docElement, "", attachmentURLs[i], "", null, 2);
        }
    }
