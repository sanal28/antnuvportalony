/* 15 jun 2017  for star rating starts */
fieldset, label {
    margin: 0;
    padding: 0;
}

/****** Style Star Rating Widget *****/

.rating, .layoutRating {
    border: none;
    float: left;
}

    .rating > input, .layoutRating > input {
        display: none;
    }

    .rating > label:before, .layoutRating > label:before {
        margin: 5px;
        font-size: 1.25em;
        font-family: FontAwesome;
        display: inline-block;
        content: "\f005";
    }

    .rating > .half:before, .layoutRating > .half:before {
        content: "\f089";
        position: absolute;
    }

    .rating > label, .layoutRating > label {
        color: rgba(189, 189, 189, 0.6);
        float: right;
    }

    .layoutRating fieldset {
        padding: 0 !important;
    }
    /***** CSS Magic to Highlight Stars on Hover *****/

    .rating > input:checked ~ label, /* show gold star when clicked */
    .rating:not(:checked) > label:hover, /* hover current star */
    .rating:not(:checked) > label:hover ~ label {
        color: #ecc64b;
    }
    /* hover previous stars in list */

    .layoutRating > input:checked ~ label {
        color: #ecc64b;
    }
/* hover previous stars in list */
.ratingdiv fieldset {
    padding: 0 !important;
}

.ratingdiv {
    padding: 0;
    margin: 0;
}

    .ratingdiv label {
        padding: 0;
        margin: 0;
        position: relative;
        top: 0px;
