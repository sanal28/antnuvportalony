/*weekly off*/
.weeklyoffmaindiv {
    padding: 0;
    width: 100%;
}

.weeklytext {
    padding: 5px 15px;
    margin: 10px 0px 0px 0px;
    font-family: 'Lato', sans-serif;
    font-size: 1em;
    color: #fff;
    width: 100%;
}

.weeklytext1 {
    padding: 5px 0px;
    margin: 10px 0px 0px 0px;
    font-family: 'Lato', sans-serif;
    font-size: 1em;
    color: #fff;
    width: 100%;
}

.weeklyoffdiv {
    width: 100%;
    height: 30px;
    border-radius: 50px;
    background-color: rgba(74,205,222,0.80);
    text-align: justify;
    -ms-text-justify: distribute-all-lines;
    text-justify: distribute-all-lines;
}

    .weeklyoffdiv > .weeklydiv {
        width: 30px;
        height: 30px;
        border-radius: 50px;
        background-color: rgba(255, 255, 255, 0.50);
        vertical-align: top;
        display: inline-block;
        *display: inline;
        zoom: 1;
    }

    .weeklyoffdiv:after {
        content: '';
        width: 100%;
        display: inline-block;
        font-size: 0;
        line-height: 0;
    }

    .weeklyoffdiv > .weeklydiv:nth-child(1) {
        background: #ff5b25;
    }

    .weeklyoffdiv > .weeklydiv:nth-child(2) {
        background-color: rgba(255, 255, 255, 0.50);
    }

    .weeklyoffdiv > .weeklydiv:nth-child(3) {
        background-color: rgba(255, 255, 255, 0.50);
    }

    .weeklyoffdiv > .weeklydiv:nth-child(4) {
        background-color: rgba(255, 255, 255, 0.50);
    }

    .weeklyoffdiv > .weeklydiv:nth-child(5) {
        background-color: rgba(255, 255, 255, 0.50);
    }

    .weeklyoffdiv > .weeklydiv:nth-child(6) {
        background-color: rgba(255, 255, 255, 0.50);
    }

    .weeklyoffdiv > .weeklydiv:nth-child(7) {
        background: #ff5b25;
    }

/*.weeklydiv{width: 30px; height: 30px; border-radius: 50px; background-color: rgba(255, 255, 255, 0.50); float: left;}*/
