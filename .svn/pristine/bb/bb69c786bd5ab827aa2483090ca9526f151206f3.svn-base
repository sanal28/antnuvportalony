@charset "UTF-8";
/* CSS Document */

body {
    margin: 0;
    padding: 0;
    /*position: relative;*/
    width: 100%;
    /*overflow: inherit;*/
    background-image: url(../Images/background-images/4e81b8475e56f173dd296e2c3d0790f8.jpg);
    background-repeat: no-repeat;
    background-size: cover;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-color: #024265 !important;
    background-position: center center;
    background-attachment: fixed;
}

.myprojecttaskname a {
    color: #fff !important;
}

    .myprojecttaskname a:hover {
        color: #fff !important;
        text-decoration: none !important;
    }

.container {
    padding: 0 !important;
    margin: auto;
    background-color: rgba(0, 127, 174, 0.4);
    /*opacity: 0.5;
    filter: alpha(opacity=50);*/
    height: 100%;
    position: relative;
}


.headerdiv {
    padding: 0;
    margin: 0;
    background-color: #7b8997;
    overflow: auto;
}

.logodiv {
    padding: 0px;
    margin: 0;
    float: left;
}

.searchmaindiv {
    padding: 3px 15px;
    margin: 8px 10px 0px 10px;
    float: right;
    background-color: #b1b8c0;
    border-radius: .5em;
}

.searchtext {
    font-family: 'Lato', sans-serif;
    font-size: .8em;
    color: #fff;
    background-color: transparent;
    border: none;
    padding: 2px;
    margin-right: 5px;
    width: 100%;
}

.logoutdiv {
    width: 16px;
    height: 16px;
    padding: 0;
    margin: 10px 20px;
    float: right;
}

.headermaindiv {
    padding: 10px;
    margin: 0;
    background-image: url(../Images/header-bg-hg.png);
    background-repeat: no-repeat;
    width: 100%;
    min-height: 100px;
}

.headertopdiv {
    padding: 5px 20px;
    margin: 0;
    overflow: auto;
}

.headeredit {
    width: 28px;
    height: 28px;
    padding: 0;
    margin-right: 10px;
    float: right;
    opacity: 0.5;
    filter: alpha(opacity=50);
}

.userprofilediv {
    padding: 0;
    margin: auto;
    width: 100px;
    height: 100px;
    border-radius: 50%;
    /*background-image: url(images/user-profile.png);*/
    background-repeat: no-repeat;
}

.userprofilename {
    padding: 5px;
    margin: 0px 0px;
    font-family: 'Lato', sans-serif;
    font-size: 1.7em;
    color: #fff;
    text-transform: uppercase;
}

.userprofiledesignation {
    padding: 5px;
    margin: 0px 0px;
    font-family: 'Lato', sans-serif;
    font-size: 1em;
    color: #fff;
    text-transform: capitalize;
}

.userprofileemail {
    padding: 5px;
    margin: 15px 0px 0px 15px;
    font-family: 'Lato', sans-serif;
    font-size: 1em;
    color: #fff;
    text-align: right;
}

.userprofileemail1 {
    padding: 5px;
    margin: 0px 15px;
    font-family: 'Lato', sans-serif;
    font-size: 1em;
    color: #fff;
    text-align: right;
}

.space {
    padding: 10px;
    margin: 0;
}

/*leftnav*/
.navleftdiv {
    background-color: #0b3546;
    opacity: 0.7;
    filter: alpha(opacity=70);
    width: 100%;
    height: 100%;
    position: relative;
    /*position: relative; bottom: 0;*/
}

.leftnavmaindiv {
    padding: 5px;
    margin: 0;
}

.leftnavicon {
    width: 32px;
    height: 32px;
    padding: 0;
    margin: 8px auto 4px auto;
}

.leftnavtext {
    padding: 0px 0px 5px 0px;
    margin: 0px 0px;
    text-align: center;
}

    .leftnavtext a {
        font-family: 'Lato', sans-serif;
        font-size: .8em;
        color: #27c3fe;
        text-align: center;
        text-decoration: none;
    }

        .leftnavtext a:hover {
            color: #fff;
            text-decoration: none;
        }

.leftnavdivider {
    width: 100%;
    height: 2px;
    background-image: url(../Images/nav-divider.png);
    background-repeat: repeat-x;
}

/*rightmaincontent*/
.mainheadingdiv {
    padding: 0;
    margin: 10px 10px 10px 0px;
    background-color: rgba(255,255,255,0.5);
}

.mainheadericondiv {
    background-color: #0b3546;
    padding: 0px;
    margin: 0px auto;
}

.mainheadericon {
    width: 45px;
    height: 45px;
    margin: auto; /*background-image: url(../Images/personal-info-icon.png); background-repeat: no-repeat; background-size: 45px 45px;*/
}

    .mainheadericon img {
        margin: auto;
    }

