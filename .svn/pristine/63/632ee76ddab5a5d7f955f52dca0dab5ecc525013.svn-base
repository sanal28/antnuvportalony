    function loadScheduleInterview(intId,subId,ticStatId) {
        $('#divReqDocs').empty();
        $("#hdnInterviewLog").val(intId);
        $("#hdnSubmissionId").val(subId);
        $("#hdnTicketStatusId").val(ticStatId);

        $.ajax({
            url: '/Home/GetSelectData',
            type: "Get",
            dataType: 'json',
            data: { Id: intId, Operation: 49 },
            success: function (result) {
                var resultVals = $.parseJSON(result);                
                if (resultVals["Result"] == undefined || resultVals["Result"] == null) {
                    $("#txtCandidateName").val(resultVals[0]["CandidateName"]).attr('disabled', 'disabled');
                    $("#txtClientName").val(resultVals[0]["ClientName"]).attr('disabled', 'disabled');
                    $("#txtPostionTitle").val(resultVals[0]["PositionTitle"]).attr('disabled', 'disabled');
                    if (resultVals[0]["FK_Interviewtype"] != 0) {
                        $("#ddlInterviewType").val(resultVals[0]["FK_Interviewtype"]);
                        $("#ddlInterviewType").click();
                        $("#ddlInterviewType").attr('disabled', 'disabled');
                    }
                    else {
                        $("#ddlInterviewType").val("").attr('disabled', 'disabled');
                    }
                    $("#txtInterviewDate").val(FormatDate(resultVals[0]["InterviewDate"],'mm/dd/yyyy')).attr('disabled', 'disabled');
                    $("#txtInterviewTime").val(resultVals[0]["InterviewTime"]).attr('disabled', 'disabled');
                    $("#txtInterviewLocation").val(resultVals[0]["Location"]).attr('disabled', 'disabled');
                    $("#txtNote").val(resultVals[0]["Note"]).attr('disabled', 'disabled');
                    showAttachments(resultVals[0]["Attachments"]);
                    $("#filehidden").val(resultVals[0]["Attachments"]);
                    $(".fileuploads").hide();
                    $(".closeImage").hide();
                    $(".hideButtons").hide();                                                                                               
                }
            },
        });

        $("#dlgInterviewSchedule")[0].showModal();
    }
