        $("#listTech").empty();
        $("#listCert").empty();
        $("#listDomain").empty();
        $("#listSkills").empty();
        $("#divAttach").empty();
        $("#listManager").empty();
        $("#listRecruit").empty();
        LoadDdls('/projects/GetDepartment?opId=7', $('#ddlDepartment'), 'DepartmentId', 'DepartmentName');
        $.ajax({
            url: '/Recruitment/GetOpening',
            dataType: "json",
            type: "GET",
            contentType: 'application/json; charset=utf-8',
            data: {
                RowId: OppId, operation: 41
            },
            success: function (data) {
                var results = $.parseJSON(data);
                if (results["Result"] == undefined || results["Result"] == null) {
                    $('#dlgPostingTitle').find('.popupheadingtext')[0].innerText = results[0].JobTitle;
                    $('#txtTitle').val(results[0].JobTitle).attr("disabled", "disabled");
                    if (results[0].FK_Department != 0)
