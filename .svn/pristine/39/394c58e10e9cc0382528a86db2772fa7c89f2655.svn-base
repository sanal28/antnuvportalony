    function showIntervieweeDialog(AppId, title) {
        var dialog = document.getElementById('dlgIntervieweeName');
        $('#listInterviewSkills').empty();
        $('#listInterviewTech').empty();
        $('#dlgIntervieweeName').find('.popupheadingtext')[0].innerText = title;
        $.ajax({
            url: '/Recruitment/GetOpening',
            dataType: "json",
            type: "GET",
            contentType: 'application/json; charset=utf-8',
            data: {
                RowId: AppId, operation: 48
            },
            success: function (data) {
                var resultVals = $.parseJSON(data);
                if (resultVals["Result"] == undefined || resultVals["Result"] == null) {
                    $('#txtIntName').val(resultVals[0].Name).attr("disabled", "disabled");
                    $('#txtEmail').val(resultVals[0].EmailId).attr("disabled", "disabled");
                    $('#txtMobile').val(resultVals[0].MobileNo).attr("disabled", "disabled");
                    $('#txtAddress').val(resultVals[0].Address).attr("disabled", "disabled");
                    $('#txtStreet').val(resultVals[0].Street).attr("disabled", "disabled");
                    $('#txtIntCity').val(resultVals[0].city).attr("disabled", "disabled");
                    $('#txtIntState').val(resultVals[0].State).attr("disabled", "disabled");
                    if (resultVals[0].ZipCode != 0)
                        $('#txtZip').val(resultVals[0].ZipCode);
                    $('#txtIntCountry').val(resultVals[0].Country).attr("disabled", "disabled");
                    if (resultVals[0].ExperianceinMonths != 0)
                        $('#txtExp').val(resultVals[0].ExperianceinMonths);
                    $('#txtQual').val(resultVals[0].HighestQualification).attr("disabled", "disabled");
                    $('#txtJob').val(resultVals[0].CurrentJobTitle).attr("disabled", "disabled");
                    $('#txtEmp').val(resultVals[0].CurrentEmployer).attr("disabled", "disabled");
                    if (resultVals[0].CurrentCTC != 0)
                        $('#txtCTCcurr').val(resultVals[0].CurrentCTC);
                    if (resultVals[0].ExpectedCTC != 0)
                        $('#txtCTCexp').val(resultVals[0].ExpectedCTC);
                    $('#txtSkype').val(resultVals[0].SkypeID).attr("disabled", "disabled");
                    $('#txtTwitt').val(resultVals[0].TwitterID).attr("disabled", "disabled");
                    $('#txtLink').val(resultVals[0].LinkedInID).attr("disabled", "disabled");
                    $('#txtPosTitle').val(resultVals[0].JobTitle).attr("disabled", "disabled");
                    bindListData($("#listInterviewTech"), resultVals[0].Technologies);
                    bindListData($("#listInterviewSkills"), resultVals[0].Skills);
                    $('#txtInfo').val(resultVals[0].AdditionalInfo).attr("disabled", "disabled");
                    $('.listclosbtn').hide();
                    $('#txtZip').attr("disabled", "disabled");
                    $('#txtExp').attr("disabled", "disabled");
                    $('#txtCTCcurr').attr("disabled", "disabled");
                    $('#txtCTCexp').attr("disabled", "disabled");
                    $('#txtSkill').attr("disabled", "disabled");
                    $('#txtTech').attr("disabled", "disabled");
                }
            },
            error: function (xhr) {
            }
        });
        dialog.showModal();
    }
    function bindListData(list, text) {
        var listText = text.split(',');
        if (listText.length > 0) {
            for (var i = 0; i < listText.length; i++)
                if (listText[i] != '')
                    list.append("<div class='div2'><div style='padding-left:2px;margin: 0; font-size: 11px; color: #fff; min-width: 50px; width:auto; float: left;'><label class='divcompetancy'>" + listText[i] +
                                    "</label></div><div class='listclosbtn' style='margin: 0; font-size: 11px; color: #fff; height: 10px;  width:10px; float: left; '>" +
                                        "<a title='Close' href='#' style='color: #fff;' onclick='this.parentNode.parentNode.parentNode.removeChild(this.parentNode.parentNode);'>X</a></div>");
        }
    }
    function showPostingTitleDialog(OppId) {
