    function viewStatus(empId,schedId,applId,status,IntAssessmentId){
        $("#hdnEmpId").val(empId);
        $("#hdnAppId").val(applId);
        $("#hdnStatusId").val(status);
        $("#divAttachpop").empty();
        if(IntAssessmentId != 0)
            $("#divCandidateRating").show();
        else
            $("#divCandidateRating").hide();
        var dialogInterStatus = document.getElementById("dlgInterStatus");
        $.ajax({
            url: '/Recruitment/CancelDelete',
            dataType: "json",
            type: "GET",
            contentType: 'application/json; charset=utf-8',
            data: {
                RowId: schedId,EmpId:empId, Operation: 10
            },
            success: function (data) {
                var results = $.parseJSON(data);
                if(results.Result==undefined||results.Result==null||results.Result!="Error"){
                    $('#txtStatuspop').val(results[0].StatusName).attr('disabled','disabled');
                    $('#txtCommentspop').val(results[0].Comments).attr('disabled','disabled');
                    var filesStr=results[0].Attachments.split('||');
                    if(filesStr.length>1){
                        for(var i=0;i<filesStr.length-1;i++)
                            CheckExtension($('#divAttachpop'), filesStr[i], filesStr[i].split('\\').pop(), "", $('#divAttachpop'), 1);
                        $('#divFilesattach').show();
                    }
                }

            }
        });
        dialogInterStatus.showModal();
    }
    function loadCandidateRating(empId,applId,status)
    {
        if(status==14){
            var dialogAssessment = document.getElementById("dlgCandidateRating");            
            $.ajax({
                url: '/Recruitment/CancelDelete',
                dataType: "json",
                type: "GET",
                contentType: 'application/json; charset=utf-8',
                data: {
                    RowId: applId, EmpId:empId,Operation: 15
                },
                success: function (data) {
                    var results = $.parseJSON(data);
                    LoadDdls('/Recruitment/BindDropdown?RowId='+results[0].InterviewerStatusId+'&Operation=24', $('#ddlApplstatus'), 'ApplicantStatusId', 'StatusName');
                    $.ajax({
                        url: '/Recruitment/CancelDelete',
                        dataType: "json",
                        type: "GET",
                        contentType: 'application/json; charset=utf-8',
                        data: {
                            RowId: results[0].CreateIntrviewId,EmpId:results[0].InterviewerId, Operation: 11
                        },
                        success: function (data) {
                            var results = $.parseJSON(data);
                            if(results.Result==undefined||results.Result==null||results.Result!="Error"){
                                if(parseInt(results[0].TechinicalAbility)>0){
                                    for(var i=1;i<=parseInt(results[0].TechinicalAbility);i++)
                                        $('#divTechAb').find('input[value="'+i+'"]').prop('checked',true);
                                }
                                if(parseInt(results[0].InterpersonalSkills)>0){
                                    for(var i=1;i<=parseInt(results[0].InterpersonalSkills);i++)
                                        $('#divInterSk').find('input[value="'+i+'"]').prop('checked',true);
                                }
                                if(parseInt(results[0].PresentaitonSkills)>0){
                                    for(var i=1;i<=parseInt(results[0].PresentaitonSkills);i++)
                                        $('#divPresentSk').find('input[value="'+i+'"]').prop('checked',true);
                                }
                                if(parseInt(results[0].LeadershipSkills)>0){
                                    for(var i=1;i<=parseInt(results[0].LeadershipSkills);i++)
                                        $('#divLeaderSk').find('input[value="'+i+'"]').prop('checked',true);
                                }
                                if(parseInt(results[0].Teamwork)>0){
                                    for(var i=1;i<=parseInt(results[0].Teamwork);i++)
                                        $('#divTeam').find('input[value="'+i+'"]').prop('checked',true);
                                }
                                $('#ddlApplstatus option').remove();
                                $('#ddlApplstatus').append('<option value="1">'+results[0].StatusName+'</option>');
                                $('#ddlApplstatus').attr('value','1');
                                $('#txtIntComments').val(results[0].Comments);
                                $('#assessRating').find('input[type="radio"]').prop('disabled',true);
                                $('#ddlApplstatus').attr('disabled','disabled');
                                $('#txtIntComments').attr('disabled','disabled');
                            }
                            PreventDefaultClick();
                        }
                    });             
                },
                error: function (xhr) {
                }
            }); 
            dialogAssessment.showModal();
        }
    }
    function UpdateInterviewStatus() {
        if (ValidateAll('divValidateAll','lblMsg')) {
            var dtStatus = [{ Applicant:$('#hdnApplicantId').val(),InterviewerId:$('#hdnInterviewerId').val(),Status:$('#ddlIntStatus option:selected').val(),Attachments:$('#filehidden').val(),Comments:$('#txtComments').val() }];
            $.ajax({
                url: '/Recruitment/UpdateInterviewStatus',
                type: 'POST',
                dataType: 'json',
                data: {
                    JsonStr: JSON.stringify(dtStatus),Operation:recruiter?1:2
                },
                success: function (data) {
                    var result = JSON.parse(data);
                    if(result.Result==undefined||result.Result==null||result.Result!="Error"){
                        $('#lblMsg').attr("class", "lblMessage");
                        $('#lblMsg')[0].innerText = "Status Updated Successfully";
                        loadScheduledInterview();
                        loadCompletedInterView();
                    }
                },
                error: function () {
                }
            });
        }
    }
    function closeInterviewStatus()
    {
        $('#txtComments').val('');
        $('#divAttach').html('');
        document.getElementById('dlgInterviewStatus').close();
    }
</script>
