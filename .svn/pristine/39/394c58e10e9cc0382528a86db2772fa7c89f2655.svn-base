                            RowId: results[0].CreateIntrviewId, operation: 74
                        },
                        success: function (data)
                        {
                            var resultValues = $.parseJSON(data);
                            for(var i=0;i<resultValues.length;i++)
                            {
                                if(resultValues[i].InterviewerStatusId == 10 || resultValues[i].InterviewerStatusId == 11 || resultValues[i].InterviewerStatusId == 12 || resultValues[i].InterviewerStatusId == 14 )
