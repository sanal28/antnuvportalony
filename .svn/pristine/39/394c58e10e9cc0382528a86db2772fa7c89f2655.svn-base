                                    "<img src='../Images/documenticon.png' onclick='viewStatus("+resultValues[i].EmpId+","+results[0].CreateIntrviewId+","+results[0].NewApplicantId+","+results[i].InterviewerStatusId+","+resultValues[i].InterviewAssessmentId+");' class='divcompetancy' name='" + resultValues[i].EmpId + "' />"+
                                    "</div><div style='margin: 0; font-size: 11px; color: #fff; height: 10px;  width:10px; float: left; '></div></div>");
                                else
                                    $('#listInterviewer').append("<div class='div2'><div style='padding-left:2px;margin: 0; font-size: 11px; color: #fff; min-width: 50px; width:auto; float: left;'><label style='cursor:pointer'>" + resultValues[i].FirstName +"</label>"+
